{
  "manifest_version": 3,
  "name": "Site Blocker",
  "version": "1.0",
  "description": "Bloqueia sites especificados pelo usu√°rio",
  "permissions": [
    "storage",
    "declarativeNetRequest"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html",
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "options.html"
  }
}
